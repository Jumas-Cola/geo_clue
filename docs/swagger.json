                },
                "country_id": {
                    "type": "integer"
                },
                "region_ru": {
                    "type": "string"
                },
                "title_ru": {
                    "type": "string"
                }
            }
        },
        "models.Country": {
            "type": "object",
            "properties": {
                "country_id": {
                    "type": "integer"
                },
                "title_ru": {
                    "type": "string"
                }
            }
        }
    }
